{
  "name": "@contacto-io/style-guide",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "git://github.com/contacto-io/contacto-console",
  "version": "0.4.5",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "npm run css-build && rollup -c",
    "css-build": "node-sass --omit-source-map-url src/style/main.scss build/style-guide.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s dist",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "chromatic": "npx chromatic --project-token=b366e708e279",
    "analyze": "source-map-explorer 'build/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@storybook/addon-actions": "^6.3.5",
    "@storybook/addon-essentials": "^6.3.5",
    "@storybook/addon-links": "^6.3.5",
    "@storybook/addons": "^6.3.5",
    "@storybook/client-api": "^6.3.12",
    "@storybook/node-logger": "^6.3.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.5",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/testing-react": "0.0.21",
    "@storybook/theming": "^6.3.5",
    "@testing-library/react": "^12.0.0",
    "antd": "^4.16.8",
    "chromatic": "^5.10.0-canary.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "loadash": "^1.0.0",
    "material-design-icons": "^3.0.1",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.6",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "4.0.3",
    "rollup": "^2.53.3",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.4",
    "simplebar": "^5.3.5",
    "simplebar-react": "^2.3.5",
    "source-map-explorer": "^2.5.2",
    "ts-dedent": "^2.1.1",
    "yeoman-generator": "^5.4.2"
  },
  "peerDependencies": {
    "antd": "^4.16.8",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "simplebar": "^5.3.5",
    "simplebar-react": "^2.3.5"
  }
}
